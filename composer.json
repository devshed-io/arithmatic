{
    "name": "devshed-io/arithmatic",
    "description": "Chainable math methods in php",
    "keywords": [
        "library",
        "maths",
        "equations",
        "fluent",
        "chainable"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Charles Bowen",
            "email": "charlesrbowen93@gmail.com"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox"
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Devshed\\Arithmatic\\": "Src/"
        },
        "files": [
            "Src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Devshed\\Arithmatic\\Tests\\": "Tests/"
        }
    }
}
